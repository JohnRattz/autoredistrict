LOAD 3 2010 2012
SET DISTRICTS ALLOW_4_SEATS TRUE
SET DISTRICTS FAIRVOTE_SEATS -1
SET EVOLUTION ELITE_FRAC 0.25
SET EVOLUTION ELITE_MUTATE_FRAC 1.0
STOP
SET POPULATION COLUMN POPULATION
SET COUNTY COLUMN COUNTY_NAM
SET WEIGHT COUNT_SPLITS TRUE
SET ETHNICITY COLUMNS VAP_WHITE VAP_BLACK VAP_HISPAN VAP_ASIAN VAP_INDIAN VAP_OTHER
SET ELECTION COLUMNS PRES12_DEM PRES12_REP
SET DISTRICTS COLUMN AR_RESULT
SET EVOLUTION ANNEAL_RATE 0.87
SET EVOLUTION MUTATE_RATE 0.87
SET WEIGHT GEOMETRY_FAIRNESS 0.22
SET EVOLUTION MUTATE_RATE 1.0

SET DISTRICTS FAIRVOTE_SEATS -1
SET DISTRICTS ALLOW_4_SEATS TRUE
SET DISTRICTS FAIRVOTE_SEATS -1
GO
STOP
SAVE
	EXPORT
	EXIT
